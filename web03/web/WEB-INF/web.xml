<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ViewDepartemenServlet</servlet-name>
        <servlet-class>servlet.ViewDepartemenServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertDepartemenServlet</servlet-name>
        <servlet-class>servlet.InsertDepartemenServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewDepartemenServlet</servlet-name>
        <url-pattern>/viewdepartemen</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertDepartemenServlet</servlet-name>
        <url-pattern>/insertdepartemen</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
